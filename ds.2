#include <stdio.h>

int hourglassSum(int arr[6][6]) {
    int max_sum = -63;  // The minimum possible value for an hourglass sum (-9 * 7 = -63)
    
    // Iterate through each possible hourglass center (from (1,1) to (4,4))
    for (int i = 1; i < 5; i++) {
        for (int j = 1; j < 5; j++) {
            // Calculate the sum of the hourglass with the center at (i, j)
            int sum = arr[i-1][j-1] + arr[i-1][j] + arr[i-1][j+1]   // top row: a, b, c
                    + arr[i][j]                                  // middle row: d
                    + arr[i+1][j-1] + arr[i+1][j] + arr[i+1][j+1]; // bottom row: e, f, g

            // Update max_sum if the current hourglass sum is greater
            if (sum > max_sum) {
                max_sum = sum;
            }
        }
    }
    
    return max_sum;
}

int main() {
    int arr[6][6];
    
    // Read the 6x6 array from input
    for (int i = 0; i < 6; i++) {
        for (int j = 0; j < 6; j++) {
            scanf("%d", &arr[i][j]);
        }
    }
    
    // Calculate and print the maximum hourglass sum
    printf("%d\n", hourglassSum(arr));
    
    return 0;
}
